[run]
branch = True
concurrency =
    multiprocessing
    thread
omit =
    betty/_package/**
parallel = True
source = betty

[report]
exclude_also =
    ^\s*pragma: no cover$
    ^\s*def __repr__
    if TYPE_CHECKING:
    @abstractmethod
    @overload
omit =
    betty/_package/**
show_missing = True
skip_covered = True
skip_empty = True
