{
  "output": "",
  "base_url": "http://example.com",
  "content_negotiation": true,
  "plugins": {
    "betty.plugin.nginx.Nginx": {
      "www_directory_path": "/var/www/betty/"
    }
  },
  "locales": [
    {
      "locale": "en-US",
      "alias": "en"
    },
    {
      "locale": "nl-NL",
      "alias": "nl"
    }
  ]
}
