{% set place = place | default(entity) %}
{% set embedded = embedded | default(False) %}
<span>
    {%- set names = place.names | select_has_dates(date_context | default(none)) | list -%}
    {%- if names | length == 0 -%}
        {%- set names = place.names -%}
    {%- endif -%}
    {%- set name = names | first | localize -%}

    {%- if not embedded -%}
        <a href="{{ place | localized_url }}">
    {%- endif -%}
    {{ name | html_lang }}
    {%- if not embedded -%}
        </a>
    {%- endif -%}
</span>
